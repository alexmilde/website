generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Review {
  id   Int    @id @default(autoincrement())
  text String @db.Text
  name String @db.Text
  rating Int @db.Int
  published Boolean @db.TinyInt @default(false)
}

model test {
  id   Int    @id @default(autoincrement())
  text String @db.Text
}
